import shutil
import os
source_path = r'C:\Users\SAIL\Downloads\image.jfif'  
destination_path = r'C:\Users\SAIL\Pictures\image.jfif'  

if os.path.exists(source_path):
    shutil.move(source_path, destination_path)
    print(f"Image moved successfully to {destination_path}")
else:
    print("Source file does not exist.")
